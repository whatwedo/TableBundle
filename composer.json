{
    "name": "whatwedo/table-bundle",
    "type": "symfony-bundle",
    "description": "Table generator for symfony",
    "license": "MIT",
    "homepage": "https://doc.whatwedo.ch/whatwedo/tablebundle",
    "keywords": [
        "symfony-ux"
    ],
    "authors": [
        {
            "name": "whatwedo GmbH",
            "email": "welove@whatwedo.ch"
        }
    ],
    "require": {
        "php":                      ">=8.1",
        "symfony/framework-bundle": "~5.3|~6.0",
        "symfony/http-kernel":      "~5.3|~6.0",
        "symfony/validator":        "~5.3|~6.0",
        "whatwedo/core-bundle":     "^1.0",
        "phpoffice/phpspreadsheet": "^1.22"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v6.0",
        "symfony/http-kernel": "^v6.0",
        "symfony/config": "^v6.0",
        "symfony/dependency-injection": "^v6.0",
        "symfony/yaml": "^v6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "nyholm/symfony-bundle-test": "^1.8",
        "doctrine/doctrine-bundle": "2.5.5",
        "doctrine/orm": "^2.11",
        "whatwedo/php-coding-standard": "dev-develop",
        "zenstruck/foundry": "^1.16",
        "zenstruck/console-test": "^v1.1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "whatwedo/core-bundle":     "dev-1.0-dev as 1.0",
        "whatwedo/search-bundle":     "dev-3.0-dev as 3.0",
        "gedmo/doctrine-extensions": "^3.5",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/security-core": "^6.0",
        "symfony/security-bundle": "^6.0"
    },
    "suggest": {
        "whatwedo/search-bundle": "Allows searching tables"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\TableBundle\\": "src/",
            "whatwedo\\TableBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-feature/query-builder-provider": "0.4.x-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "twbs/icons",
                "version": "1.6.1",
                "source": {
                    "url": "https://github.com/twbs/icons",
                    "type": "git",
                    "reference": "tags/v1.6.1"
                }
            }
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
